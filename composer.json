{
    "name": "demidovich/d2-data-mapper",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "php",
        "data mapper",
        "ddd",
        "domain driven design"
    ],
    "autoload": {
        "psr-4": {
            "D2\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Performance\\": "performance/"
        }
    },
    "require": {
        "php": ">=7.4",
        "demidovich/d2-hydrator": "^0.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=8.0",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/var-dumper": "^5.1"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/demidovich/d2-entity-builder"
        },
        {
            "type": "vcs",
            "url": "https://github.com/demidovich/d2-hydrator"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
